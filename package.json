{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Simple react-webpack boilerplate",
  "main": "index.js",
  "repository": "https://github.com/eriolo/react-webpack-boilerplate.git",
  "author": "eriolo <eol0204@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "npm run clean && npm run copy && webpack-dev-server --config webpack.config.js --env.release=false",
    "start:build": "npm run clean && npm run copy && webpack-dev-server --config webpack.config.js --env.release=true",
    "copy": "mkdir build && cp src/index.html build/",
    "clean": "rm -Rf build"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "webpack-dev-server": "^2.7.1"
  }
}
